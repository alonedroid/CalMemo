<LinearLayout
    android:id="@+id/cm_calendar_root"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/cm_text_color"
    tools:context="alonedroid.com.calmemo.scene.calendar.CmCalendarFragment_"
    >
    <LinearLayout
        android:id="@+id/cm_calendar_1"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        style="@style/CmCalendarRow"
        />
    <LinearLayout
        android:id="@+id/cm_calendar_2"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        style="@style/CmCalendarRow"
        />
    <LinearLayout
        android:id="@+id/cm_calendar_3"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        style="@style/CmCalendarRow"
        />
    <LinearLayout
        android:id="@+id/cm_calendar_4"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        style="@style/CmCalendarRow"
        />
    <LinearLayout
        android:id="@+id/cm_calendar_5"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        style="@style/CmCalendarRow"
        />
    <LinearLayout
        android:id="@+id/cm_calendar_6"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:orientation="horizontal"
        style="@style/CmCalendarRow"
        />
</LinearLayout>